{
    "name": "mui-carousel-react",
    "version": "1.0.1",
    "description": "A Generic, extendible Carousel UI component for React using Material UI",
    "keywords": [
        "react",
        "material",
        "material ui",
        "carousel",
        "gallery"
    ],
    "homepage": "https://learus.github.io/react-material-ui-carousel",
    "bugs": {
        "url": "https://github.com/gazi786/mui-carousel/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gazi786/mui-carousel.git"
    },
    "license": "MIT",
    "author": "AG",
    "type": "commonjs",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "/dist"
    ],
    "scripts": {
        "build": "npx rimraf dist && npm run build:cjs",
        "build:cjs": "tsc --module commonjs --outDir dist",
        "build_old": "NODE_ENV=production && npx rimraf dist && npx mkdirp dist && npx babel ./src --out-dir dist --copy-files"
    },
    "dependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@gazi786/mui-carousel": "file:",
        "@mui/icons-material": "^6.4.7",
        "@mui/material": "^6.4.7",
        "@mui/system": "^6.4.7",
        "framer-motion": "^12.5.0",
        "mui-carousel": "file:",
        "mui-carousel-react": "file:",
        "react-material-ui-carousel": "file:",
        "react-mui-carousel": "file:"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^0.0.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.13.0",
        "gh-pages": "^6.3.0",
        "react-scripts": "^3.0.1",
        "typescript": "^5.8.2"
    },
    "peerDependencies": {
        "@emotion/react": "^11.4.1",
        "@emotion/styled": "^11.3.0",
        "@mui/icons-material": "^6.4.7",
        "@mui/material": "^6.4.7",
        "@mui/system": "^6.4.7",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "module": "dist/index.js",
    "babel": {
        "presets": [
            "react",
            "env",
            "stage-2"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}